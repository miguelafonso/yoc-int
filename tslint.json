{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "member-access": false,
    "no-debugger": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "strictNullChecks": false,
    "interface-name": false
  }
}
